{
  "name": "dance-roster-api",
  "version": "1.0.0",
  "description": "",
  "main": "build/main.js",
  "scripts": {
    "test": "jest",
    "test:watch": "npm run test -- --watch",
    "start": "NODE_PATH=./.build node .build/server.js",
    "build": "npm run clean && tsc -p . && npm run remove:typings",
    "dev": "nodemon src/server.ts",
    "lint": "eslint . --ext .js --ext .ts",
    "lint:fix": "npm run lint -- --fix",
    "prettier-fix": "prettier --write \"src/**/*.ts\"",
    "code:check": "lint:fix; npm run prettier-fix",
    "clean": "rimraf .build",
    "remove:typings": "npm rum remove:migrations && npm run remove:seeders",
    "remove:migrations": "rimraf -v .build/infrastructure/migrations/*.d.ts",
    "remove:seeders": "rimraf -v .build/infrastructure/seeders/*.d.ts",
    "seed:up": "npm run .build && npx sequelize-cli db:seed:all",
    "seed:undo": "npm run .build && npx sequelize-cli db:seed:undo",
    "migrate:up": "npm run .build && npx sequelize-cli db:migrate",
    "migrate:undo": "npm run .build && npx sequelize-cli db:migrate:undo"
  },
  "author": "Ramses Banco (@ramBlanco)",
  "license": "ISC",
  "devDependencies": {
    "@commitlint/cli": "^17.4.4",
    "@commitlint/config-conventional": "^17.4.4",
    "@types/aws-lambda": "^8.10.137",
    "@types/bcryptjs": "^2.4.2",
    "@types/express": "^4.17.17",
    "@types/i18n": "^0.13.6",
    "@types/jest": "^29.5.0",
    "@types/morgan": "^1.9.4",
    "@types/node": "^18.15.0",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.55.0",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.7.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-functional": "^5.0.6",
    "eslint-plugin-import": "^2.27.5",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "lint-staged": "^13.2.0",
    "nodemon": "^2.0.21",
    "prettier": "^2.8.4",
    "sequelize-cli": "^6.6.2",
    "serverless-offline": "^13.3.3",
    "serverless-plugin-typescript": "^2.1.5",
    "supertest": "^6.3.3",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.4.3"
  },
  "dependencies": {
    "@fastify/autoload": "^5.8.0",
    "@fastify/awilix": "^5.1.0",
    "@fastify/aws-lambda": "^4.0.0",
    "@fastify/jwt": "^8.0.0",
    "@fastify/rate-limit": "^9.1.0",
    "@fastify/swagger": "^8.14.0",
    "@prisma/client": "^5.12.0",
    "@types/aws-sdk": "^2.7.0",
    "awilix": "^10.0.1",
    "aws-sdk": "^2.1333.0",
    "bcryptjs": "^2.4.3",
    "dotenv": "^16.0.3",
    "fastify": "^4.26.2",
    "fastify-plugin": "^4.5.1",
    "fluent-json-schema": "^4.2.1",
    "i18n": "^0.15.1",
    "morgan": "^1.10.0",
    "pg": "^8.11.5",
    "pg-hstore": "^2.3.4",
    "prisma": "^5.12.0",
    "sequelize": "^6.29.3",
    "sequelize-typescript": "^2.1.6"
  }
}
