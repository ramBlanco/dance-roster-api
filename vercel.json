{
  "framework": null,
  "buildCommand": "npm run build",
  "installCommand": "npm install",
  "outputDirectory": "build",
  "functions": {
    "build/serverless.js": {
      "includeFiles": "build/**"
    }
  },
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "api/serverless.ts"
    }
  ]
}