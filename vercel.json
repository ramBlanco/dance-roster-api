{
  "buildCommand": "npm run build",
  "installCommand": "npm install",
  "outputDirectory": "build",
  "builds": [
    {
      "src": "build/serverless.js",
      "use": "@vercel/node",
      "config": {
        "includeFiles": [
          "build/**"
        ]
      }
    }
  ],
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "build/serverless.js"
    }
  ]
}