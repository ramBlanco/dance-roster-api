{
  "version": 2,
  "builds": [
    {
      "src": "build/serverless.js",
      "use": "@vercel/node",
      "config": {
        "includeFiles": [
          "build/**"
        ]
      },
      "buildCommand":"npm run build"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "build/serverless.js"
    }
  ]
}